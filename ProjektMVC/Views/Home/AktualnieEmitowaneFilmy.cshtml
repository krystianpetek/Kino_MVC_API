@model IEnumerable<ProjektMVC.Models.AktualnieEmitowaneFilmy>

@{
    ViewData["Title"] = "Aktualnie emitowane filmy";
}
<div class="text-center">
    <h1 class="display-4">Aktualnie emitowane filmy</h1>
    <br />
</div>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Data)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Godzina)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NazwaFilmu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Data)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Godzina)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NazwaFilmu)
            </td>
        </tr>
}
    </tbody>
</table>
